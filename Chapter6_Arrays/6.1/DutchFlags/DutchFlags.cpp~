using namespace std;

typedef enum {BLACK,WHITE,RED} Color;

void DutchFlags(vector<Color> &flags){

}

int main(){
  vector<Color> flags = {BLACK,WHITE,RED,BLACK,WHITE,RED,BLACK,BLACK,RED};
